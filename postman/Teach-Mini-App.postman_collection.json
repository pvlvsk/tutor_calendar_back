                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"initData\": \"{{initData}}\"\n}"
                        },
                        "url": {
                            "raw": "{{baseUrl}}/auth/init",
